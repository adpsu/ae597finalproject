import pandas as pd

# Sample DataFrame
data = {
    'make': ['Toyota', 'Ford', 'Chevrolet', 'BMW', 'Tesla'],
    'model': ['Camry', 'F-150', 'Malibu', 'X5', 'Model 3'],
    'MSRP': [24000, 0, 22000, 0, 35000],
    'vehicle_type': ['Sedan', 'Truck', 'Sedan', 'SUV', 'Sedan']
}
df = pd.DataFrame(data)
msrp_zero = df[df['MSRP'] == 0]
print(msrp_zero)
for index, row in msrp_zero.iterrows():
    print(f"Updating MSRP for {row['make']} {row['model']}")
    new_msrp = float(input(f"Enter new MSRP for {row['make']} {row['model']}: "))
    df.at[index, 'MSRP'] = new_msrp
